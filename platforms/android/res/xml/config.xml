<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Cordova.Ionic.GoogleMapPlugin" version="2.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="HttpRequest">
        <param name="android-package" value="plugin.http.request.HttpRequest" />
    </feature>
    <feature name="GoogleMaps">
        <param name="android-package" value="plugin.google.maps.GoogleMaps" />
    </feature>
    <feature name="External">
        <param name="android-package" value="plugin.google.maps.External" />
    </feature>
    <feature name="Geocoder">
        <param name="android-package" value="plugin.google.maps.MyGeocoder" />
    </feature>
    <name>Google Map Plugin for Ionic Framework</name>
    <description>
    Sample working app using the Google Maps plugin with Ionic Framework.
  </description>
    <author email="" href="">
	</author>
    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <gap:plugin name="com.adobe.plugins.gaplugin" />
    <gap:plugin name="dk.interface.cordova.plugin.googlenavigate" version="0.1.2" />
    <gap:plugin name="org.apache.cordova.camera" version="0.3.2" />
    <gap:plugin name="org.apache.cordova.file-transfer" version="0.4.6" />
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.13" />
    <gap:plugin name="nl.x-services.plugins.socialsharing" version="4.3.8" />
    <gap:plugin name="plugin.google.maps.menusito" version="1.2.5" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="true" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-installLocation" value="auto" />
    <icon src="cordova/cordova_128.png" />
    <icon gap:density="ldpi" gap:platform="android" src="cordova/cordova_android_36.png" />
    <icon gap:density="mdpi" gap:platform="android" src="cordova/cordova_android_48.png" />
    <icon gap:density="hdpi" gap:platform="android" src="cordova/cordova_android_72.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="cordova/cordova_android_96.png" />
    <icon gap:platform="ios" height="57" src="cordova/cordova_ios_57.png" width="57" />
    <icon gap:platform="ios" height="72" src="cordova/cordova_ios_72.png" width="72" />
    <icon gap:platform="ios" height="114" src="cordova/cordova_ios_114.png" width="114" />
    <icon gap:platform="ios" height="144" src="cordova/cordova_ios_144.png" width="144" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="cordova/res/screen/android_ldpi_portrait.png.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="cordova/res/screen/android_mdpi_portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="cordova/res/screen/android_hdpi_portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="cordova/res/screen/android_xhdpi_portrait.png" />
    <gap:splash gap:platform="blackberry" src="cordova/res/screen/blackberry_transparent_300.png" />
    <gap:splash gap:platform="ios" height="480" src="cordova/res/screen/iphone_portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="cordova/res/screen/iphone_retina_portrait.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="cordova/res/screen/ipad_portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="cordova/res/screen/ipad_landscape.png" width="1024" />
    <gap:splash gap:platform="winphone" src="cordova/res/screen/windows_phone_portrait.jpg" />
    <access browserOnly="true" origin="*" subdomains="true" />
</widget>
